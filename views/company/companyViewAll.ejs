<html>
<head>
    <title>Companies</title>
</head>
<body>
<h1>Companies</h1>
<a href="/company/add">Add New Company</a> <br />

<br />

<table border="1">
    <tr><th>Company Name</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><a href="/company/?company_id=<%= result[i].company_id %>"><%= result[i].company_name %></a></td>
    </tr>
    <% } %>
</table>
</body>

</html>