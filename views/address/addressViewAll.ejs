<html>
<head>
    <title>Addresses</title>
</head>
<body>
<h1>Addresses</h1>
<a href="/address/add">Add New Address</a> <br />

<br />

<table border="1">
    <tr><th>Street</th><th>Zip Code</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><a href="/address/?address_id=<%= result[i].address_id %>"><%= result[i].street %></a></td>
        <td><%= result[i].zip_code%></td>
    </tr>
    <% } %>
</table>
</body>

</html>