<%- include("../includes/header.ejs", {title:"Add New account"}); %>
<h1>Update User <%= result[0].first_name + " " + result[0].last_name %>'s Account </h1>
<form action="/account/update" method="get">
    <input type="hidden" id="account_id" name="account_id" value="<%= result[0].account_id %>">
    First Name:
    <br />
    <input id="first_name" name="first_name" type="text" value="<%= result[0].first_name%>"/>
    <br />
    <br />
    Last Name:
    <br />
    <input id="last_name" name="last_name" type="text" value="<%= result[0].last_name%>"/>
    <br />
    <br />
    Email:
    <br />
    <input id="email" name="email" type="text" value="<%= result[0].email %>"/>
    <br />
    <br />
    <input type="submit" />
</form>
<%- include("../includes/footer.ejs"); %>