<html>
    <head>
        <title>Resumes</title>
    </head>
    <body>
        <h1>Resumes</h1>
        <a href="/resume/add">Add New Resume</a> <br />

        <br />

        <table border="1">
            <tr><th>User Account ID</th><th>Resume Name</th></tr>
            <% for(var i=0; result.length > i; i++ ) { %>
            <tr>
                <td><a href="/resume"<%= result[i].user_account_id %></td>
                <td><a href="/resume/?resume_id=<%= result[i].resume_id %>"><%= result[i].resume_name %></a></td>

            </tr>
            <% } %>
        </table>
    </body>

</html>